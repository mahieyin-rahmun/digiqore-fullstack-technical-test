{
  "name": "digiqore-fullstack-test",
  "version": "1.0.0",
  "description": "Digiqore Full Stack Technical Test",
  "main": "server.js",
  "scripts": {
    "format": "npx prettier --write ./src",
    "start": "npm run build && cross-env NODE_ENV=production node dist/server.js",
    "start:dev": "cross-env NODE_ENV=development nodemon dist/server.js",
    "build": "swc src -d dist --source-maps --copy-files",
    "build:dev": "swc src -d dist --source-maps --copy-files -w",
    "run:dev": "cross-env NODE_ENV=development concurrently \"npm run build:dev\" \"npm run start:dev\"",
    "test": "jest --forceExit --detectOpenHandles",
    "test:dev": "cross-env NODE_ENV=test jest --watch --coverage --verbose --testTimeout=30000 --detectOpenHandles",
    "migrate:dev": "cross-env NODE_ENV=development npx sequelize-cli db:migrate",
    "migrate:dev:undo:last": "cross-env NODE_ENV=development npx sequelize-cli db:migrate:undo",
    "migrate:dev:undo": "cross-env NODE_ENV=development npx sequelize-cli db:migrate:undo:all",
    "migrate:test": "cross-env NODE_ENV=test npx sequelize-cli db:migrate",
    "migrate:test:undo": "cross-env NODE_ENV=test npx sequelize-cli db:migrate:undo:all",
    "seed:dev": "cross-env NODE_ENV=development npx sequelize-cli db:seed",
    "seed:dev:all": "cross-env NODE_ENV=development npx sequelize-cli db:seed:all",
    "seed:dev:undo": "cross-env NODE_ENV=development npx sequelize-cli db:seed:undo",
    "seed:dev:undo:all": "cross-env NODE_ENV=development npx sequelize-cli db:seed:undo:all",
    "seed:test": "cross-env NODE_ENV=test npx sequelize-cli db:seed:all",
    "seed:test:undo": "cross-env NODE_ENV=test npx sequelize-cli db:seed:undo:all"
  },
  "keywords": [
    "fullstack",
    "react",
    "nodejs",
    "express"
  ],
  "author": "Mahieyin Rahmun",
  "license": "ISC",
  "devDependencies": {
    "@jest/globals": "^29.0.3",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/hpp": "^0.2.2",
    "@types/jest": "^29.0.3",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.18",
    "@types/selenium-webdriver": "^4.1.5",
    "@types/supertest": "^2.0.12",
    "jest": "^29.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.2",
    "@types/dotenv": "^8.2.0",
    "compression": "^1.7.4",
    "concurrently": "^7.4.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "helmet": "^6.0.0",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.2.0",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "morgan": "^1.10.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "selenium-webdriver": "^4.4.0",
    "sequelize": "^6.23.1",
    "sequelize-cli": "^6.4.1",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  }
}
